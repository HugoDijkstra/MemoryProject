<Application x:Class="MemoryProjectFull.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MemoryProjectFull"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <Style x:Key="GlobalStyle" TargetType="{x:Type Control}">
            <Setter Property="FontFamily" Value="pack://application:,,,/assets/fonts/font.ttf#MedievalSharp"/>
        </Style>

    </Application.Resources>
</Application>
